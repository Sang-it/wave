class Rectangle{
    constructor(length) {
        this.length = length;
    }
    getArea() {
        return this.length ** 2;
    }
}

class Cube extends Rectangle{
    constructor(length, height) {
        super(length);
        this.height = height;
    }
    getVolume() {
        return this.getArea() * this.height;
    }
}

