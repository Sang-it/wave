function addRecur(number){
    if (number == 0) return 0;
    return number + addRecur(number - 1);
}

function fibonacci(number){
    if (number == 1) return 0;
    if (number == 2) return 1;
    return fibonacci(number - 1) + fibonacci(number - 2);
}

